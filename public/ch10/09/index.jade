h1 My Latest Dives
.row
  .col-sm-4.col-sm-offset-8
    input.form-control.input-lg(placeholder="Search" ng-model='searchKey')
.row(ng-controller='diveLogCtrl')
  .col-sm-4(
    ng-repeat="dive in dives"
      + " | filter: {site: searchKey, location: '!' + searchKey}"
  )
    h3 {{dive.site}}
    h3 {{dive.location}}
    h3 {{dive.depth}} feet | {{dive.time}} min
