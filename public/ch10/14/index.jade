h1 My Latest Dives
.row
  .col-sm-4.col-sm-offset-8
    input.form-control.input-lg(
      type='number'
      placeholder="Limit to items"
      ng-model='limitValue'
    )
.row
  .col-sm-4(ng-repeat="dive in dives | limitTo: limitValue")
    h3 {{dive.site}}
    h3 {{dive.location}}
    h3 {{dive.depth}} feet | {{dive.time}} min
