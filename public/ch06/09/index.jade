h1 My Latest Dives (ng-class)
.row
    .col-sm-6
      select.form-control(
        ng-model='selection'
        ng-options='item.id as item.name for item in options'
      )
.row
  div(ng-class="size()",ng-repeat="dive in dives")
    .card(ng-class="color()")
      h3 {{dive.site}}
      h4 {{dive.location}}
      h2 {{dive.depth}} feet | {{dive.tiime}} min
