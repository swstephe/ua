style.
  .logEntry2 {
    background-color: #eeeeee;
    border: 2px solid darkgray;
    padding: 2px 16px;
  }
  .logEntry2 > h2 {
    color: black;
  }
  .logEntry2 > h3 {
    color: gray;
  }
  .logEntry2 > h4 {
    color: darkgray;
  }
.logEntry2
  h2 {{dive.depth}} feet | {{dive.time}} min
  h3 {{dive.site}}
  h4 {{dive.location}}
