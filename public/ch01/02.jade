doctype html
html
  head
    title Dive Log (HTML)
    link(rel='stylesheet',href='/bootstrap/css/bootstrap.css')
  body
    .container-fluid
      h1 My Latest Dives (HTML)
      #logbook.row
    script(src='/jquery-2.1.4.js')
    script(src='dives.js')
    script
      :coffee-script
        $(document).ready ->
          for dive in dives
            $('#logbook').append """
              <div class='col-sm-4'>
              <h3>#{dive.site}</h3>
              <h4>#{dive.location}</h4>
              <h2>#{dive.depth} feet | #{dive.time} min</h2>
              </div>
            """
