- var dives = JSON.stringify(public.ch01._data.dives).replace(/"/g,"'")
doctype html
html(ng-app)
  head
    title Dive Log (AngularJS)
    link(rel='stylesheet',href='/bootstrap/css/bootstrap.css')
  body
    .container-fluid
      h1 My Latest Dives (AngularJS)
      .row(ng-init='dives='+dives)
        .col-sm-4(ng-repeat='dive in dives')
          h3 {{dive.site}}
          h4 {{dive.location}}
          h2 {{dive.depth}} feet | {{dive.time}} min
    script(src='/angular/angular.js')
